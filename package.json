    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-testing-library": "^4.6.0",
    "husky": "^6.0.0",
    "jest-axe": "^5.0.1",
    "lint-staged": "^11.0.0",
    "msw": "^0.29.0",
    "prettier": "^2.3.0",
    "prettier-stylelint": "^0.4.2",
    "stylelint": "^13.13.1",
    "stylelint-a11y": "^1.2.3",
    "stylelint-config-standard": "^22.0.0",
    "stylelint-order": "^4.1.0"
